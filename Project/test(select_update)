define table test having fields(test1 boolean, test2 char(3), test3 date, test4 integer, test5 real, test6 varchar);
insert (true, 'dog', '11/12/1994', 19, 3.14159, 'Huntington') into test;
insert (false, 'cat', '01/10/1997', 24, 2.90, 'Potato') into test;
insert (true, 'pen', '07/04/1776', 35, 7.25, 'Jacob') into test;
insert (true, 'lid', '12/21/2012', 92, 2.71828, 'Computer') into test;
insert (false, 'jaw', '04/02/2014', 24, 0.00001, 'Word') into test;
print test;
update test set test4 = 25 where test4 = 24;
select test where test4 != 25;
update test set test1 = false where test2 = 'pen';
select test where test1 = false;
update test set test6 = 'Potato';
select test;
drop table test;
exit;